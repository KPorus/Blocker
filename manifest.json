{
  "manifest_version": 3,
  "name": "URL Blocker",
  "version": "1.0",
  "description": "Automatically extract URLs from Google search results",
  "permissions": ["activeTab", "tabs","webNavigation"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
